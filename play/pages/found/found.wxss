.found-item {
  background: white;
  padding-top: 30rpx;
}
.found-bigType {
  font-weight: bold;
  display: block;
  border-bottom: 1rpx solid #E1E1E2;
  padding: 20rpx;
}
.found-type {
  font-size: 35rpx;
  display: block;
  color: #888;
  font-weight: bold;
  padding: 30rpx;
  border-bottom: 1rpx solid #E1E1E2;
}
.found-item-item-item{
  border-bottom: 1rpx solid #E1E1E2;
}
.found-item-item-item-pic {
  width: 150rpx;
  height: 150rpx;
  margin: 30rpx;
}
.found-item-item-item-detail {
  width: 70%;
}
.found-item-item-item-detail-title {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  margin-top: -35rpx;
}
.found-item-item-item-detail-ins {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  font-size: 25rpx;
  margin-top: -25rpx;
  color: #555;
}
.found-item-item-item-detail-time {
  font-size: 25rpx;
  color: #888;
}
.toTop {
  position: fixed;
  bottom: 50rpx;
  right: 50rpx;
  border: 1rpx solid #ccc;
  background: white;
  width:80rpx;
  height: 80rpx;
  padding: 30rpx 30rpx;
  text-align: center;
  border-radius: 15rpx;
}